    function OnSubmitBusForm()
    {
    if(document.pressed == 'Schedule')
    {
    document.find_route.action ="!{httpPrefix}//metro.kingcounty.gov/cftemplates/show_schedule.cfm";
    }
    else
    if(document.pressed == 'Map')
    {
    document.find_route.action ="!{httpPrefix}//metro.kingcounty.gov/cftemplates/show_map7.cfm";
    }
    return true;
    }
    (function( $ ){

      $.fn.deliciousNewsFeedHome = function( options ) {  
        // Create some defaults, extending them with any options that were provided
        var settings = $.extend( {
          feedURL      : 'http://feeds.delicious.com/v2/json/kingcounty',
          numItems     : 3,
        }, options);

        //Set global scope for instance
        var $this = this,
            output = ""

        return this.each(function() {
          var dataURL = settings.feedURL + '?count=' + settings.numItems;
          $.ajax({
            url: dataURL,
            dataType: 'jsonp',
            }).
            success(function(data) {
              parseData(data);
            })
        });
          
        function parseData(data) {
          var counter = 0;
          output += '<ul>';
          $.each (data, function(i, item) {
            //Start HTML string
            if (item.d.split(': ')[1] === undefined) {
              return;
            }
            var titleStr = item.d;
            var titleParts = titleStr.split(': ');
            var titleStrSub = titleParts[1];
            //Get Event name
            output += '<li>'
            output +='<a href="'+item.u+'" title="'+ item.d +'">' + titleStrSub + '</a>';
            //Wrap up
            output+='</li>';
          });
          output+= '</ul>'
          output+= '<a href="http://www.kingcounty.gov"><img src="data:image/jpeg;base64," /></a>';
          $this.append(output);
        }
      };
    })( jQuery );
    $(".carousel-news-item").deliciousNewsFeedHome({}); 